mixin main-nav(selected)

  -
    var menu = {
      'главная': 'index.html',
      'мои работы': 'error-page.html',
      'обо мне': 'error-page.html',
      'контакты': 'error-page.html',
    };
  button(type="button").main-nav__btn меню
  nav.main-nav
    ul.main-nav__list
      each val, key in menu
        li(class= selected.toLowerCase() === key.toLowerCase() ? 'main-nav__item  main-nav__item--active' : 'main-nav__item')
          a(href=val, title=key)= key
